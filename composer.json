{
    "name": "renegadik/botality",
    "description": "Multi-platform bot framework for Telegram, Discord, etc.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "renegadik\\botality\\": "src/"
        }
    },
    "authors": [
        {
            "name": "renegadik",
            "email": "renegadik@example.com"
        }
    ],
    "minimum-stability": "dev",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.6",
        "symfony/http-client": "7.3.x-dev",
        "nyholm/psr7": "^1.8@dev",
        "longman/telegram-bot": "v1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "keywords": ["bot", "telegram", "discord", "framework"],
    "homepage": "https://github.com/renegadik/botality-php",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
